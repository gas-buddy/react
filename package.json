{
  "name": "@gasbuddy/react",
  "version": "8.0.2",
  "description": "GasBuddy Platform for Web Projects",
  "main": "build/index.js",
  "module": "build-webpack/index.js",
  "sideEffects": false,
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=@babel/register tests/test_*.js",
    "ci-test": "cross-env NODE_ENV=test nyc tap -Rtap tests/test_*.js | tee .tap_output | tap -Rspec -",
    "ci-cover": "nyc check-coverage",
    "ci-coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "prepublish": "npm run build && npm run webpack",
    "build": "babel --source-maps inline src/ --out-dir build/",
    "webpack": "cross-env NODE_ENV=production BABEL_ENV=webpack babel src/ --out-dir build-webpack/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gas-buddy/web.git"
  },
  "keywords": [
    "web",
    "react"
  ],
  "author": "Max Metral",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/gas-buddy/web/issues"
  },
  "homepage": "https://github.com/gas-buddy/web#readme",
  "dependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@loadable/component": "^5.10.1",
    "babel-plugin-transform-react-class-to-function": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.8",
    "react-router-dom": "^5.0.0",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-preset-gasbuddy": "^5.5.0",
    "cross-env": "^5.2.0",
    "eslint-config-gasbuddy": "^5.2.1",
    "redux-logger": "^3.0.6",
    "tap": "^14.1.10"
  }
}
